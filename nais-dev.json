{
  "apiVersion": "nais.io/v1alpha1",
  "kind": "Application",
  "metadata": {
    "name": "soknadsveiviser",
    "namespace": "default",
    "labels": {
      "team": "personbruker"
    }
  },
  "spec": {
    "image": "navikt/soknadsveiviser:LATEST",
    "team": "personbruker",
    "port": 8080,
    "ingresses": [
      "https://soknader.nais.oera-q.local/soknader",
      "https://www-q0.nav.no/soknader"
    ],
    "liveness": {
      "path": "/soknader/internal/alive",
      "initialDelay": 10
    },
    "readiness": {
      "path": "/soknader/internal/ready",
      "initialDelay": 10
    },
    "secrets": true,
    "replicas": {
      "min": 2,
      "max": 4
    },
    "webproxy": true,
    "resources": {
      "limits": {
        "cpu": "3",
        "memory": "768Mi"
      },
      "requests": {
        "memory": "512Mi"
      }
    }
  }
}
